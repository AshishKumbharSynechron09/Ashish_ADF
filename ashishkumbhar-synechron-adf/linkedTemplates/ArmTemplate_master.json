{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "ashishkumbhar-synechron-adf"
		},
		"LinkService_AzureBlobStorage_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LinkService_AzureBlobStorage'"
		},
		"LinkService_DataLake_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'LinkService_DataLake'"
		},
		"LinkService_Databricks_accessToken": {
			"type": "secureString",
			"metadata": "Secure string for 'accessToken' of 'LinkService_Databricks'"
		},
		"LinkedService_AzureSQLServer_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LinkedService_AzureSQLServer'"
		},
		"LinkService_DataLake_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://datalakeadftrainings.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ashishkumbhar-synechron-adf_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"LinkService_AzureBlobStorage_connectionString": {
						"value": "[parameters('LinkService_AzureBlobStorage_connectionString')]"
					},
					"LinkService_DataLake_accountKey": {
						"value": "[parameters('LinkService_DataLake_accountKey')]"
					},
					"LinkService_Databricks_accessToken": {
						"value": "[parameters('LinkService_Databricks_accessToken')]"
					},
					"LinkedService_AzureSQLServer_connectionString": {
						"value": "[parameters('LinkedService_AzureSQLServer_connectionString')]"
					},
					"LinkService_DataLake_properties_typeProperties_url": {
						"value": "[parameters('LinkService_DataLake_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}